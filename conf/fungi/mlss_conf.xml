<?xml version="1.0" encoding="UTF-8"?>
<compara_db division="fungi">

  <!-- Collections are species-sets that are needed to define several method-link species sets. -->
  <collections>
    <collection name="default">
      <taxonomic_group taxon_name="Eukaryota"/>
    </collection>

  </collections>

  <pairwise_alignments>
    <all_vs_all method="LASTZ_NET">
      <species_set>
       <genome name="eremothecium_gossypii_fdag1_gca_000968835"/>
       <genome name="fusarium_oxysporum"/>
       <genome name="fusarium_solani"/>
       <genome name="fusarium_verticillioides"/>
       <genome name="magnaporthe_poae"/>
       <genome name="puccinia_graminis"/>
       <genome name="puccinia_triticina"/>
       <genome name="pyricularia_oryzae"/>
       <genome name="saccharomyces_cerevisiae"/>
       <genome name="schizosaccharomyces_cryophilus"/>
       <genome name="schizosaccharomyces_japonicus"/>
       <genome name="schizosaccharomyces_octosporus"/>
       <genome name="schizosaccharomyces_pombe"/>
       <genome name="trichoderma_virens"/>
      </species_set>
    </all_vs_all>
  </pairwise_alignments>

  <gene_trees>
    <protein_trees collection="default"/>
  </gene_trees>

</compara_db>
