    'species_tree_file' => '/home/user/production/compara_homology_54/hive/compara_homology_54/spec_tax.nh',
    'honeycomb_dir' => '/home/user/production/compara_homology_54/hive/54/h'
  },

  { TYPE => dNdS,
    'codeml_parameters' => do('/home/user/production/compara_homology_54/hive/compara_homology_54/codeml.ctl.hash'),
    'species_sets' => '[[3,22,31,38,39,46,57,60,64],[42,87,88],[4,65]]',
    'method_link_types'=>'["ENSEMBL_ORTHOLOGUES"]'
  },

# You can specify multiple custom binary locations when you are running the
# pipeline as with GENE_TREE. Available options are:
#   * program_file (Slr binary location)
#   * gblocks_exe

